@model IEnumerable<AD419.DataHelper.Web.Models.SfnClassificationLogic>

@{
    ViewBag.Title = "Index - SFN Classification Logic";
}

<h2>@ViewBag.Title</h2>

<p>
    <h5>@Html.ActionLink("View SFN CASE Statement", "Display") generated by the following entries.</h5>
</p>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EvaluationOrder)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ParameterOrder)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubParameterOrder)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LogicalOperator)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ColumnName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NegateCondition)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ConditionalOperator)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Values)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SFN)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EvaluationOrder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ParameterOrder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SubParameterOrder)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LogicalOperator)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ColumnName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NegateCondition)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ConditionalOperator)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Values)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SFN)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}
</table>
