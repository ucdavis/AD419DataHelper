        <th>
            @Html.DisplayNameFor(model => model.OrganizationShortName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CrisDepartmentCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OrganizationShortCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountEndCharacters)
