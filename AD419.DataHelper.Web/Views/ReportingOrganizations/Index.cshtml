@model IEnumerable<AD419.DataHelper.Web.Models.ReportingOrganization>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
